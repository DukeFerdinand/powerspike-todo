{
  "compilerOptions": {
    "outDir": "./built/",
    "sourceMap": true,
    "strict": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": ["es2018", "dom"],
    "plugins": [
      {
        "name": "ts-vue-plugin"
      }
    ]
  },
  "include": ["./src/**/*"]
}
